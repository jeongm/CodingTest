   "source": [
    "N = int(input())\n",
    "suger = -1\n",
    "\n",
    "for i in range(N//5,-1,-1):\n",
    "    n = N\n",
    "    n -= 5*i\n",
    "    n -= n//3*3\n",
    "    if n == 0:\n",
    "        suger = i + (N-5*i)//3\n",
    "        break\n",
    "    \n",
    "print(suger)"
   ]
